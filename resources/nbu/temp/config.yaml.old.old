---
server:
    host: "0.0.0.0"
    port: <PORT>
    uri: "/metrics"
    scrappingInterval: "15m"
    logName: "nbu-exporter.log"
nbuserver:
    scheme: "https"
    uri: "/netbackup"
    domain: "<Your_NBU_Domain>"
    domainType: "NT"
    host: "<Your_NBU_Host>"
    port: 1556
    apiKey: "<Your_apiKey>"
    contentType: "application/vnd.netbackup+json; version=3.0"